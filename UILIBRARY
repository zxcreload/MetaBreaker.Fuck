local elements={}
local elementCounter=0
function elements:Button(options)
if not options or not options.text or not options.callback then
Notify("Button","Missing arguments!")
return
end
elementCounter=elementCounter+1
local elementId="Button_"..options.text.."_"..elementCounter
local TextButton=Instance.new("TextButton")
TextButton.Parent=sectionFrame
TextButton.BackgroundColor3=Colors.DarkGreen
TextButton.Size=UDim2.new(0,200,0,22)
TextButton.AutoButtonColor=false
TextButton.Text=options.text
TextButton.Font=Enum.Font.Gotham
TextButton.TextColor3=Colors.LightGreen
TextButton.TextSize=14
TextButton.BackgroundTransparency=1
ApplyButtonEffect(TextButton)
ApplyClickEffect(TextButton,5)
TextButton.MouseButton1Click:Connect(function()
options.callback()
ElementStates[elementId]={type="Button",clicked=true}
SaveConfig()
end)
Resize(25)
end
function elements:Toggle(options)
if not options or not options.text or not options.callback then
Notify("Toggle","Missing arguments!")
return
end
elementCounter=elementCounter+1
local elementId="Toggle_"..options.text.."_"..elementCounter
local State=options.state or false
if ElementStates[elementId]then
State=ElementStates[elementId].state
end
local toggleLabel=Instance.new("TextLabel")
toggleLabel.Name="toggleLabel"
toggleLabel.Parent=sectionFrame
toggleLabel.BackgroundTransparency=1
toggleLabel.Size=UDim2.new(0,200,0,22)
toggleLabel.Font=Enum.Font.Gotham
toggleLabel.Text=" "..options.text
toggleLabel.TextColor3=State and Colors.WhiteGlow or Colors.LightGreen
toggleLabel.TextSize=14
toggleLabel.TextXAlignment=Enum.TextXAlignment.Left
ApplyButtonEffect(toggleLabel)
local toggleFrame=Instance.new("TextButton")
toggleFrame.Name="toggleFrame"
toggleFrame.Parent=toggleLabel
toggleFrame.BackgroundColor3=State and Colors.Black or Colors.DarkGreen
toggleFrame.AnchorPoint=Vector2.new(0.5,0.5)
toggleFrame.Position=UDim2.new(0.9,0,0.5,0)
toggleFrame.Size=UDim2.new(0,38,0,18)
toggleFrame.AutoButtonColor=false
toggleFrame.Text=""
Instance.new("UICorner",toggleFrame).CornerRadius=UDim.new(0,50)
Instance.new("UIStroke",toggleFrame).Color=Colors.WhiteGlow
local toggleButton=Instance.new("TextButton")
toggleButton.Name="toggleButton"
toggleButton.Parent=toggleFrame
toggleButton.BackgroundColor3=State and Colors.LightGreen or Colors.Black
toggleButton.AnchorPoint=Vector2.new(0.5,0.5)
toggleButton.Position=State and UDim2.new(0.74,0,0.5,0)or UDim2.new(0.25,0,0.5,0)
toggleButton.Size=UDim2.new(0,16,0,16)
toggleButton.AutoButtonColor=false
toggleButton.Text=""
Instance.new("UICorner",toggleButton).CornerRadius=UDim.new(0,50)
local function PerformToggle()
State=not State
options.callback(State)
ApplyTween(toggleButton,{
Position=State and UDim2.new(0.74,0,0.5,0)or UDim2.new(0.25,0,0.5,0),
BackgroundColor3=State and Colors.LightGreen or Colors.Black
})
ApplyTween(toggleLabel,{
TextColor3=State and Colors.WhiteGlow or Colors.LightGreen
})
ApplyTween(toggleFrame,{
BackgroundColor3=State and Colors.Black or Colors.DarkGreen
})
ElementStates[elementId]={type="Toggle",state=State}
SaveConfig()
end
toggleFrame.MouseButton1Click:Connect(PerformToggle)
toggleButton.MouseButton1Click:Connect(PerformToggle)
ElementStates[elementId]={type="Toggle",state=State}
Resize(25)
end
function elements:Slider(options)
if not options or not options.text or not options.min or not options.max or not options.callback then
Notify("Slider","Missing arguments!")
return
end
elementCounter=elementCounter+1
local elementId="Slider_"..options.text.."_"..elementCounter
local Slider=Instance.new("Frame")
Slider.Name="Slider"
Slider.Parent=sectionFrame
Slider.BackgroundTransparency=1
Slider.Size=UDim2.new(0,200,0,22)
local sliderLabel=Instance.new("TextLabel")
sliderLabel.Name="sliderLabel"
sliderLabel.Parent=Slider
sliderLabel.AnchorPoint=Vector2.new(0.5,0.5)
sliderLabel.BackgroundTransparency=1
sliderLabel.Position=UDim2.new(0.2,0,0.5,0)
sliderLabel.Size=UDim2.new(0,77,0,22)
sliderLabel.Font=Enum.Font.Gotham
sliderLabel.Text=" "..options.text
sliderLabel.TextColor3=Colors.LightGreen
sliderLabel.TextSize=14
sliderLabel.TextXAlignment=Enum.TextXAlignment.Left
ApplyButtonEffect(sliderLabel)
local sliderFrame=Instance.new("TextButton")
sliderFrame.Name="sliderFrame"
sliderFrame.Parent=sliderLabel
sliderFrame.BackgroundColor3=Colors.Black
sliderFrame.AnchorPoint=Vector2.new(0.5,0.5)
sliderFrame.Position=UDim2.new(1.6,0,0.5,0)
sliderFrame.Size=UDim2.new(0,72,0,2)
sliderFrame.Text=""
sliderFrame.AutoButtonColor=false
local sliderBall=Instance.new("TextButton")
sliderBall.Name="sliderBall"
sliderBall.Parent=sliderFrame
sliderBall.AnchorPoint=Vector2.new(0.5,0.5)
sliderBall.BackgroundColor3=Colors.LightGreen
sliderBall.Position=UDim2.new(0,0,0.5,0)
sliderBall.Size=UDim2.new(0,14,0,14)
sliderBall.AutoButtonColor=false
sliderBall.Text=""
Instance.new("UICorner",sliderBall).CornerRadius=UDim.new(0,50)
Instance.new("UIStroke",sliderBall).Color=Colors.WhiteGlow
local sliderTextBox=Instance.new("TextBox")
sliderTextBox.Name="sliderTextBox"
sliderTextBox.Parent=sliderLabel
sliderTextBox.BackgroundColor3=Colors.Black
sliderTextBox.AnchorPoint=Vector2.new(0.5,0.5)
sliderTextBox.Position=UDim2.new(2.4,0,0.5,0)
sliderTextBox.Size=UDim2.new(0,31,0,15)
sliderTextBox.Font=Enum.Font.Gotham
sliderTextBox.Text=tostring(options.min)
sliderTextBox.TextColor3=Colors.WhiteGlow
sliderTextBox.TextSize=11
sliderTextBox.TextWrapped=true
local Held=false
local Value=options.min
local percentage=0
local step=0.01
if ElementStates[elementId]then
Value=ElementStates[elementId].value
sliderTextBox.Text=tostring(Value)
percentage=(Value-options.min)/(options.max-options.min)*0.9
sliderBall.Position=UDim2.new(percentage,0,0.5,0)
end
local function UpdateSlider()
local MousePos=UserInputService:GetMouseLocation().X
local FrameSize=sliderFrame.AbsoluteSize.X
local FramePos=sliderFrame.AbsolutePosition.X
local pos=math.floor(((MousePos-FramePos)/FrameSize)/step+0.5)*step
percentage=math.clamp(pos,0,0.9)
Value=((options.max-options.min)/0.9)*percentage+options.min
Value=Round(Value,options.float or 1)
Value=math.clamp(Value,options.min,options.max)
sliderTextBox.Text=Value
options.callback(Value)
sliderBall.Position=UDim2.new(percentage,0,0.5,0)
ElementStates[elementId]={type="Slider",value=Value}
SaveConfig()
end
sliderFrame.MouseButton1Down:Connect(function()
Held=true
end)
sliderBall.MouseButton1Down:Connect(function()
Held=true
end)
RunService.RenderStepped:Connect(function()
if Held then
UpdateSlider()
end
end)
UserInputService.InputEnded:Connect(function(input)
if input.UserInputType==Enum.UserInputType.MouseButton1 then
Held=false
end
end)
sliderTextBox.Focused:Connect(function()
ApplyTween(sliderLabel,{TextColor3=Colors.WhiteGlow})
end)
sliderTextBox.FocusLost:Connect(function(enter)
ApplyTween(sliderLabel,{TextColor3=Colors.LightGreen})
if enter and sliderTextBox.Text~=""then
local num=tonumber(sliderTextBox.Text)
if num then
if num>options.max then
num=options.max
elseif num<options.min then
num=options.min
end
sliderTextBox.Text=tostring(num)
Value=num
options.callback(num)
percentage=(Value-options.min)/(options.max-options.min)*0.9
sliderBall.Position=UDim2.new(percentage,0,0.5,0)
ElementStates[elementId]={type="Slider",value=Value}
SaveConfig()
end
end
end)
ElementStates[elementId]={type="Slider",value=Value}
Resize(25)
end
