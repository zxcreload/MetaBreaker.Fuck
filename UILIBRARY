local Library={}
local NeverloseVersion="v1.1A"
local TweenService=game:GetService("TweenService")
local UserInputService=game:GetService("UserInputService")
local RunService=game:GetService("RunService")
local StarterGui=game:GetService("StarterGui")
local Players=game:GetService("Players")
local Workspace=game:GetService("Workspace")
local HttpService=game:GetService("HttpService")
local FastTweenInfo=TweenInfo.new(0.06,Enum.EasingStyle.Quart,Enum.EasingDirection.InOut)
local ElasticTweenInfo=TweenInfo.new(0.15,Enum.EasingStyle.Elastic,Enum.EasingDirection.Out)
for _,v in ipairs(game:GetService("CoreGui"):GetChildren())do if v:IsA("ScreenGui")and v.Name=="Neverlose"then v:Destroy()end end
local LocalPlayer=Players.LocalPlayer
local Mouse=LocalPlayer:GetMouse()
local Colors={
DarkGreen=Color3.fromRGB(20,40,20),
Black=Color3.fromRGB(10,10,10),
LightGreen=Color3.fromRGB(50,150,50),
WhiteGlow=Color3.fromRGB(200,200,200)
}
local ConfigSystem={}
local ConfigFolderName="NeverloseConfigs"
local ConfigFileName="default_config.txt"
local ElementStates={}
local ConfigFolder
local function InitializeConfigFolder()
ConfigFolder=Workspace:FindFirstChild(ConfigFolderName)
if not ConfigFolder then
ConfigFolder=Instance.new("Folder")
ConfigFolder.Name=ConfigFolderName
ConfigFolder.Parent=Workspace
end
end
local FileIOSupported=pcall(function()
writefile("test.txt","test")
delfile("test.txt")
end)
local function WriteConfigFallback(data)
local configValue=ConfigFolder:FindFirstChild("ConfigData")
if not configValue then
configValue=Instance.new("StringValue")
configValue.Name="ConfigData"
configValue.Parent=ConfigFolder
end
configValue.Value=data
end
local function ReadConfigFallback()
local configValue=ConfigFolder:FindFirstChild("ConfigData")
if configValue then
return configValue.Value
end
return nil
end
local function SaveConfig()
local configData={}
for elementId,state in pairs(ElementStates)do
configData[elementId]=state
end
local dataString=HttpService:JSONEncode(configData)
if FileIOSupported then
local path=ConfigFolderName.."/"..ConfigFileName
writefile(path,dataString)
else
WriteConfigFallback(dataString)
end
end
local function LoadConfig()
local dataString
if FileIOSupported then
local path=ConfigFolderName.."/"..ConfigFileName
if isfile(path)then
dataString=readfile(path)
end
else
dataString=ReadConfigFallback()
end
if dataString then
local success,configData=pcall(function()
return HttpService:JSONDecode(dataString)
end)
if success then
for elementId,state in pairs(configData)do
ElementStates[elementId]=state
end
end
end
end
local function Notify(title,text)
StarterGui:SetCore("SendNotification",{
Title=title,
Text=text,
Duration=5
})
end
local function Dragify(frame,parent)
parent=parent or frame
local dragging=false
local dragInput,mousePos,framePos
frame.InputBegan:Connect(function(input)
if input.UserInputType==Enum.UserInputType.MouseButton1 then
dragging=true
mousePos=input.Position
framePos=parent.Position
local connection
connection=input.Changed:Connect(function()
if input.UserInputState==Enum.UserInputState.End then
dragging=false
connection:Disconnect()
end
end)
end
end)
frame.InputChanged:Connect(function(input)
if input.UserInputType==Enum.UserInputType.MouseMovement then
dragInput=input
end
end)
UserInputService.InputChanged:Connect(function(input)
if input==dragInput and dragging then
local delta=input.Position-mousePos
parent.Position=UDim2.new(framePos.X.Scale,framePos.X.Offset+delta.X,framePos.Y.Scale,framePos.Y.Offset+delta.Y)
end
end)
end
local function Round(num,bracket)
bracket=bracket or 1
local a=math.floor(num/bracket+(num>=0 and 0.5 or-0.5))*bracket
return a<0 and a+bracket or a
end
local function ApplyButtonEffect(frame)
frame.MouseEnter:Connect(function()
if frame.TextColor3~=Colors.WhiteGlow then
TweenService:Create(frame,FastTweenInfo,{TextColor3=Colors.WhiteGlow}):Play()
end
end)
frame.MouseLeave:Connect(function()
if frame.TextColor3~=Colors.LightGreen and frame.TextColor3~=Colors.WhiteGlow then
TweenService:Create(frame,FastTweenInfo,{TextColor3=Colors.LightGreen}):Play()
end
end)
end
local function ApplyClickEffect(button,amount)
button.MouseButton1Click:Connect(function()
local originalSize=button.TextSize
local newSize=originalSize-amount
TweenService:Create(button,ElasticTweenInfo,{TextSize=newSize}):Play()
task.wait(0.1)
TweenService:Create(button,ElasticTweenInfo,{TextSize=originalSize}):Play()
end)
end
local function ApplyTween(obj,properties)
TweenService:Create(obj,FastTweenInfo,properties):Play()
end
function Library:Toggle(value)
local neverlose=game:GetService("CoreGui"):FindFirstChild("Neverlose")
if not neverlose then return end
neverlose.Enabled=value==nil and not neverlose.Enabled or value
end
