            tabIcon.BackgroundTransparency, tabIcon.BorderSizePixel, tabIcon.Position, tabIcon.Size, tabIcon.Image, tabIcon.ImageColor3 = 1, 0, UDim2.new(0.05, 0, 0.2, 0), UDim2.new(0, 24, 0, 24), options.icon, Color3.fromRGB(255, 105, 180)
            
